{"ast":null,"code":"/* eslint-disable */\nimport { BaiduMap, BmWalking, BmInfoWindow, BmBoundary } from \"vue-baidu-map-3x\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  components: {\n    BaiduMap,\n    BmWalking\n  },\n  methods: {\n    handleMapClick(event) {\n      const point = new BMap.Point(event.point.lng, event.point.lat);\n      const geoc = new BMap.Geocoder();\n\n      // 调用百度地图 API 的逆地理编码方法获取地名\n      geoc.getLocation(point, result => {\n        if (result) {\n          const address = result.address; // 地名\n          const lng = result.point.lng; // 经度\n          const lat = result.point.lat; // 纬度\n\n          // 处理获取到的地名、经纬度数据\n          console.log('地名:', address);\n          console.log('经度:', lng);\n          console.log('纬度:', lat);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["BaiduMap","BmWalking","BmInfoWindow","BmBoundary","ref","onMounted","components","methods","handleMapClick","event","point","BMap","Point","lng","lat","geoc","Geocoder","getLocation","result","address","console","log"],"sources":["/Users/mac/Desktop/作业/微服务架构/Microservice-Personal-Project/src/components/map.vue"],"sourcesContent":["<template>\n  <div class=\"map\">\n    <baidu-map class=\"map\" :center=\"{ lng: 121.474, lat: 31.230}\" :zoom=\"15\" @click= \"handleMapClick\" >\n      <bm-scale anchor=\"BMAP_ANCHOR_TOP_RIGHT\"></bm-scale>\n      <bm-walking start=\"{ lng: 121.474, lat: 31.230 }\" end=\"同济大学\" :auto-viewport=\"true\" location=\"上海\"></bm-walking>\n    </baidu-map>\n  </div>\n</template>\n\n<script>\n/* eslint-disable */\nimport { BaiduMap , BmWalking,BmInfoWindow, BmBoundary} from \"vue-baidu-map-3x\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  components: {\n    BaiduMap,\n    BmWalking\n  },\n  methods: {\n    handleMapClick(event) {\n      const point = new BMap.Point(event.point.lng, event.point.lat);\n      const geoc = new BMap.Geocoder();\n      \n      // 调用百度地图 API 的逆地理编码方法获取地名\n      geoc.getLocation(point, (result) => {\n        if (result) {\n          const address = result.address; // 地名\n          const lng = result.point.lng; // 经度\n          const lat = result.point.lat; // 纬度\n\n          // 处理获取到的地名、经纬度数据\n          console.log('地名:', address);\n          console.log('经度:', lng);\n          console.log('纬度:', lat);\n        }\n      });\n    }\n  }\n};\n</script>\n\n<style>\n.map {\n  width: 70%;\n  height: 950px;\n}\n</style>"],"mappings":"AAUA;AACA,SAASA,QAAO,EAAIC,SAAS,EAACC,YAAY,EAAEC,UAAU,QAAO,kBAAkB;AAC/E,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,eAAe;EACbC,UAAU,EAAE;IACVN,QAAQ;IACRC;EACF,CAAC;EACDM,OAAO,EAAE;IACPC,cAAcA,CAACC,KAAK,EAAE;MACpB,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAACC,KAAK,CAACH,KAAK,CAACC,KAAK,CAACG,GAAG,EAAEJ,KAAK,CAACC,KAAK,CAACI,GAAG,CAAC;MAC9D,MAAMC,IAAG,GAAI,IAAIJ,IAAI,CAACK,QAAQ,CAAC,CAAC;;MAEhC;MACAD,IAAI,CAACE,WAAW,CAACP,KAAK,EAAGQ,MAAM,IAAK;QAClC,IAAIA,MAAM,EAAE;UACV,MAAMC,OAAM,GAAID,MAAM,CAACC,OAAO,EAAE;UAChC,MAAMN,GAAE,GAAIK,MAAM,CAACR,KAAK,CAACG,GAAG,EAAE;UAC9B,MAAMC,GAAE,GAAII,MAAM,CAACR,KAAK,CAACI,GAAG,EAAE;;UAE9B;UACAM,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,OAAO,CAAC;UAC3BC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAER,GAAG,CAAC;UACvBO,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEP,GAAG,CAAC;QACzB;MACF,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}