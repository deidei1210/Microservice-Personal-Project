{"ast":null,"code":"/* eslint-disable */\nimport { BaiduMap, BmWalking, BmInfoWindow, BmBoundary, BmScale } from \"vue-baidu-map-3x\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  components: {\n    BaiduMap,\n    BmWalking\n  },\n  data() {\n    return {\n      center: {\n        lat: 39.9,\n        lng: 116.4\n      },\n      zoom: 7\n    };\n  },\n  methods: {\n    handleMapClick(event) {\n      const point = new BMap.Point(event.point.lng, event.point.lat);\n      const geoc = new BMap.Geocoder();\n\n      // 调用百度地图 API 的逆地理编码方法获取地名\n      geoc.getLocation(point, result => {\n        if (result) {\n          const address = result.address; // 地名\n          const lng = result.point.lng; // 经度\n          const lat = result.point.lat; // 纬度\n\n          // 处理获取到的地名、经纬度数据\n          console.log('地名:', address);\n          console.log('经度:', lng);\n          console.log('纬度:', lat);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["BaiduMap","BmWalking","BmInfoWindow","BmBoundary","BmScale","ref","onMounted","components","data","center","lat","lng","zoom","methods","handleMapClick","event","point","BMap","Point","geoc","Geocoder","getLocation","result","address","console","log"],"sources":["/Users/mac/Desktop/作业/微服务架构/Microservice-Personal-Project/src/components/map.vue"],"sourcesContent":["<template>\n  <div class=\"map\">\n    <baidu-map class=\"map\" :center=\"{ lng: 121.474, lat: 31.230}\" :zoom=\"zoom\" @click= \"handleMapClick\" >\n      <bm-scale anchor=\"BMAP_ANCHOR_TOP_RIGHT\"></bm-scale>\n      <bm-walking start=\"{ lng: 121.474, lat: 31.230 }\" end=\"同济大学\" :auto-viewport=\"true\" location=\"上海\"></bm-walking>\n    </baidu-map>\n  </div>\n</template>\n\n<script>\n/* eslint-disable */\nimport { BaiduMap , BmWalking,BmInfoWindow, BmBoundary,BmScale} from \"vue-baidu-map-3x\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  components: {\n    BaiduMap,\n    BmWalking\n  },\n  data() {\n    return {\n      center: { lat: 39.9, lng: 116.4 },\n      zoom: 7\n    }\n  },\n  methods: {\n    handleMapClick(event) {\n      const point = new BMap.Point(event.point.lng, event.point.lat);\n      const geoc = new BMap.Geocoder();\n      \n      // 调用百度地图 API 的逆地理编码方法获取地名\n      geoc.getLocation(point, (result) => {\n        if (result) {\n          const address = result.address; // 地名\n          const lng = result.point.lng; // 经度\n          const lat = result.point.lat; // 纬度\n\n          // 处理获取到的地名、经纬度数据\n          console.log('地名:', address);\n          console.log('经度:', lng);\n          console.log('纬度:', lat);\n        }\n      });\n    }\n  }\n};\n</script>\n\n<style>\n.map {\n  width: 70%;\n  height: 950px;\n}\n</style>"],"mappings":"AAUA;AACA,SAASA,QAAO,EAAIC,SAAS,EAACC,YAAY,EAAEC,UAAU,EAACC,OAAO,QAAO,kBAAkB;AACvF,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,eAAe;EACbC,UAAU,EAAE;IACVP,QAAQ;IACRC;EACF,CAAC;EACDO,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QAAEC,GAAG,EAAE,IAAI;QAAEC,GAAG,EAAE;MAAM,CAAC;MACjCC,IAAI,EAAE;IACR;EACF,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACC,KAAK,EAAE;MACpB,MAAMC,KAAI,GAAI,IAAIC,IAAI,CAACC,KAAK,CAACH,KAAK,CAACC,KAAK,CAACL,GAAG,EAAEI,KAAK,CAACC,KAAK,CAACN,GAAG,CAAC;MAC9D,MAAMS,IAAG,GAAI,IAAIF,IAAI,CAACG,QAAQ,CAAC,CAAC;;MAEhC;MACAD,IAAI,CAACE,WAAW,CAACL,KAAK,EAAGM,MAAM,IAAK;QAClC,IAAIA,MAAM,EAAE;UACV,MAAMC,OAAM,GAAID,MAAM,CAACC,OAAO,EAAE;UAChC,MAAMZ,GAAE,GAAIW,MAAM,CAACN,KAAK,CAACL,GAAG,EAAE;UAC9B,MAAMD,GAAE,GAAIY,MAAM,CAACN,KAAK,CAACN,GAAG,EAAE;;UAE9B;UACAc,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEF,OAAO,CAAC;UAC3BC,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEd,GAAG,CAAC;UACvBa,OAAO,CAACC,GAAG,CAAC,KAAK,EAAEf,GAAG,CAAC;QACzB;MACF,CAAC,CAAC;IACJ;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}