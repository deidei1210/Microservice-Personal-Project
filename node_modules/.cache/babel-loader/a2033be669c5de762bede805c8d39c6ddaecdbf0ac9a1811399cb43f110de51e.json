{"ast":null,"code":"/* eslint-disable */\nimport { BaiduMap, BmWalking, BmInfoWindow, BmBoundary, BmScale, BmMarker } from \"vue-baidu-map-3x\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  components: {\n    BaiduMap,\n    BmWalking\n  },\n  data() {\n    return {\n      center: {\n        lat: 39.9,\n        lng: 116.4\n      },\n      zoom: 15\n    };\n  },\n  methods: {\n    handleMapClick(event) {\n      const point = new BMap.Point(event.point.lng, event.point.lat);\n      const geoc = new BMap.Geocoder();\n\n      // 调用百度地图 API 的逆地理编码方法获取地名\n      geoc.getLocation(point, result => {\n        if (result) {\n          const address = result.address; // 地名\n          const lng = result.point.lng; // 经度\n          const lat = result.point.lat; // 纬度\n\n          // 处理获取到的地名、经纬度数据\n          console.log(\"地名:\", address);\n          console.log(\"经度:\", lng);\n          console.log(\"纬度:\", lat);\n        }\n      });\n    },\n    zoomIn() {\n      this.zoom += 1; // 放大地图缩放级别\n      // this.$refs.baiduMap.setZoom(this.zoom); // 调用 setZoom 方法更新地图缩放级别\n    },\n\n    zoomOut() {\n      this.zoom -= 1; // 缩小地图缩放级别\n      // this.$refs.baiduMap.setZoom(this.zoom); // 调用 setZoom 方法更新地图缩放级别\n    }\n  }\n};","map":{"version":3,"names":["BaiduMap","BmWalking","BmInfoWindow","BmBoundary","BmScale","BmMarker","ref","onMounted","components","data","center","lat","lng","zoom","methods","handleMapClick","event","point","BMap","Point","geoc","Geocoder","getLocation","result","address","console","log","zoomIn","zoomOut"],"sources":["/Users/mac/Desktop/作业/微服务架构/Microservice-Personal-Project/src/components/map.vue"],"sourcesContent":["<template>\n  <div class=\"map\">\n    <baidu-map\n      class=\"map\"\n      :center=\"{ lng: 121.474, lat: 31.23 }\"\n      :zoom=\"zoom\"\n      @click=\"handleMapClick\"\n    >\n      <!-- 给特殊地点加上一些点 -->\n      <bm-marker\n        :position=\"{ lng: 116.404, lat: 39.915 }\"\n        :dragging=\"true\"\n        @click=\"infoWindowOpen\"\n      >\n        <bm-info-window :show=\"show\" @close=\"infoWindowClose\" @open=\"infoWindowOpen\"\n          >我爱北京天安门</bm-info-window\n        >\n      </bm-marker>\n      <div class=\"zoom-controls\">\n        <button @click=\"zoomIn\">放大</button>\n        <button @click=\"zoomOut\">缩小</button>\n      </div>\n    </baidu-map>\n  </div>\n</template>\n\n<script>\n/* eslint-disable */\nimport {\n  BaiduMap,\n  BmWalking,\n  BmInfoWindow,\n  BmBoundary,\n  BmScale,\n  BmMarker,\n} from \"vue-baidu-map-3x\";\nimport { ref, onMounted } from \"vue\";\nexport default {\n  components: {\n    BaiduMap,\n    BmWalking,\n  },\n  data() {\n    return {\n      center: { lat: 39.9, lng: 116.4 },\n      zoom: 15,\n    };\n  },\n  methods: {\n    handleMapClick(event) {\n      const point = new BMap.Point(event.point.lng, event.point.lat);\n      const geoc = new BMap.Geocoder();\n\n      // 调用百度地图 API 的逆地理编码方法获取地名\n      geoc.getLocation(point, (result) => {\n        if (result) {\n          const address = result.address; // 地名\n          const lng = result.point.lng; // 经度\n          const lat = result.point.lat; // 纬度\n\n          // 处理获取到的地名、经纬度数据\n          console.log(\"地名:\", address);\n          console.log(\"经度:\", lng);\n          console.log(\"纬度:\", lat);\n        }\n      });\n    },\n    zoomIn() {\n      this.zoom += 1; // 放大地图缩放级别\n      // this.$refs.baiduMap.setZoom(this.zoom); // 调用 setZoom 方法更新地图缩放级别\n    },\n    zoomOut() {\n      this.zoom -= 1; // 缩小地图缩放级别\n      // this.$refs.baiduMap.setZoom(this.zoom); // 调用 setZoom 方法更新地图缩放级别\n    },\n  },\n};\n</script>\n\n<style>\n.map {\n  width: 70%;\n  height: 950px;\n}\n</style>"],"mappings":"AA2BA;AACA,SACEA,QAAQ,EACRC,SAAS,EACTC,YAAY,EACZC,UAAU,EACVC,OAAO,EACPC,QAAQ,QACH,kBAAkB;AACzB,SAASC,GAAG,EAAEC,SAAQ,QAAS,KAAK;AACpC,eAAe;EACbC,UAAU,EAAE;IACVR,QAAQ;IACRC;EACF,CAAC;EACDQ,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE;QAAEC,GAAG,EAAE,IAAI;QAAEC,GAAG,EAAE;MAAM,CAAC;MACjCC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,